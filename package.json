    "@payloadcms/plugin-form-builder": "^3.2.2",
    "@payloadcms/plugin-seo": "^3.2.2",
    "@payloadcms/richtext-slate": "^3.2.2",
    "@payloadcms/ui": "^3.2.2",
    "@radix-ui/react-avatar": "^1.1.2",
    "@radix-ui/react-checkbox": "^1.1.3",
    "@radix-ui/react-dialog": "^1.1.5",
